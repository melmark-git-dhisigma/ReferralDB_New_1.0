
.slides_control {
    height: 25px;
    left: -111px !important;
    position: relative;
    margin-top: 6px;
    width: 315px !important;
}

.slides_container {
    width: 20%;
    min-width: 100px;
    font-size: 10px;
    font-family: Arial, Helvetica, sans-serif;
    text-decoration: none;
    float: left;
    /*padding: 0 0 0 2%;*/
    margin: 4px 0 0 4%;
}

    .slides_container div.slide {
        width: 105px;
        height: 25px;
        float: left;
        /*margin: 0 1px;*/
        display: block;
    }

.item {
    float: left;
    width: 15px;
    height: 15px;
    margin: 0 5px;
}

    .item a, .item a:link, .item a:visited {
        background: none repeat scroll 0 0 #008000;
        color: #FFFFFF;
        display: block;
        font-size: 13px;
        font-weight: bold;
        height: 18px;
        padding-top: 3px;
        text-align: center;
        text-decoration: none;
        width: 20px;
        border-radius: 2px;
    }

        .item a:hover {
            background: #0053a1;
            color: #fff;
        }

.pagination {
    list-style: none;
    margin: 0;
    padding: 0;
    font-size: 1px;
    display: none;
}

    .pagination .current a {
        color: red;
        font-size: 1px;
    }

.next {
    background: url("../Images/arro.png") no-repeat scroll left top rgba(0, 0, 0, 0);
    color: #3375B4;
    display: block;
    float: left;
    font-size: 1px;
    height: 11px;
    margin: 73px 40px 0 31px;
    padding: 4px 0 0 1px;
    position: absolute;
    top: 116px;
    width: 7px;
    z-index: 2;
}

    .next:hover {
        background-position: 0 -15px;
        color: #a8a8a8;
    }

.topbarCo {
    background-color: #fefcfc;
    border-bottom: 2px solid #C3C0C0;
    height: 43px;
    padding-right: 25px;
}

.selContainer {
    background-color: green;
}

.prev {
    background: url("../Images/arro.png") no-repeat scroll -8px top rgba(0, 0, 0, 0);
    color: #3375B4;
    display: block;
    float: right;
    font-size: 1px;
    height: 11px;
    left: 24%;
    margin: 73px 167px 0 135px;
    padding: 4px 0 0 1px;
    position: absolute;
    top: 116px;
    width: 7px;
    z-index: 2;
}

    .prev:hover {
        background-position: 0 -15px;
        background: url("../Images/arro.png")-8px -15px no-repeat;
        color: #a8a8a8;
    }

.HeadingGreen {
    background: #0092b0; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top, #0092b0 0%, #006eb2 52%, #0092b0 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#0092b0), color-stop(52%,#006eb2), color-stop(100%,#0092b0)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #0092b0 0%,#006eb2 52%,#0092b0 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #0092b0 0%,#006eb2 52%,#0092b0 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, #0092b0 0%,#006eb2 52%,#0092b0 100%); /* IE10+ */
    background: linear-gradient(to bottom, #0092b0 0%,#006eb2 52%,#0092b0 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#0092b0', endColorstr='#0092b0',GradientType=0 ); /* IE6-8 */
    border-bottom: 1px solid #B2CCCA;
    font-size: 18px;
    font-weight: bold;
    margin: 1px auto 0;
    text-align: left;
    width: 100%;
}

    .HeadingGreen a {
        background: url("../../Images/lbtn.PNG") no-repeat scroll 6px 8px rgba(0, 0, 0, 0);
        color: #FFF;
        font-family: Arial,Helvetica,sans-serif;
        font-size: 12px;
        font-weight: bold;
        height: 20px;
        padding: 7px 0 1px 27px;
        text-decoration: none;
        z-index: 999;
    }

    .HeadingGreen:hover {
        background-color: #f7b515;
        color: #FFF;
    }

