<script type="text/javascript">
    function backToFun() {

        var findClass = $('#page_navigation').find('.page_link');

        var totalPages = findClass.length;
        var currentPageNo = $('#page_navigation').find('.active_page').html();
        if (totalPages > 1) {
            if (currentPageNo == 1) {
                $('.previous_link').hide();
                $('.next_link').show();
            }

            else if (currentPageNo == totalPages) {
                $('.previous_link').show();
                $('.next_link').hide();
            }
            else if (!(currentPageNo == 1 && currentPageNo == totalPages)) {
                $('.previous_link').show();
                $('.next_link').show();
            }
        }
        else if (totalPages <= 1) {
            $('.previous_link').hide();
            $('.next_link').hide();
        }
    }

    $(document).ready(function () {
        var currentPageNo = $('#page_navigation').find('.active_page').html();
        var findClass = $('#page_navigation').find('.page_link');
        var totalPages = findClass.length;

        if (currentPageNo == 1) {

            $('.previous_link').hide();
            if (totalPages == 1) {
                $('.next_link').hide();
            }

        }


    });
</script>
